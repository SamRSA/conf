{
"vim.easymotion": true,
"vim.sneak": true,
"vim.incsearch": true,
"vim.useSystemClipboard": true,
"vim.useCtrlKeys": true,
"vim.hlsearch": true,
"vim.normalModeKeyBindingsNonRecursive": [
	{
	"before": ["<leader>", "d"],
	"after": ["d", "d"]
	}
],
"vim.leader": "<space>",
"vim.handleKeys": {
	"<C-a>": false,
	"<C-f>": false,
	"<C-w>": false,
	"<C-n>": false
},
// $ command takes newline
"vim.visualModeKeyBindingsNonRecursive": [
    {
        "before": ["$"],
        "after": ["g", "_"]
    },
    {
        "before": ["g", "_"],
        "after": ["$"]
    }
],
}
